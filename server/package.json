{
  "name": "throttling-server",
  "version": "1.2.1",
  "private": true,
  "description": "throttling-server",
  "main": "index.ts",
  "scripts": {
    "start": "tsc && node dist/src/index.js",
    "build": "tsc",
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "lint": "eslint src/**/*.ts --fix",
    "test": "jest --maxWorkers=2 src",
    "prettier": "prettier --write '{src,test}/**/*.{js,ts,json,md}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint && yarn test",
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "testRegex": "src/.*\\.(test|spec).(ts)$",
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!src/**/*.test.{ts}"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-redis": "^0.0.16",
    "@types/express-jwt": "^6.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.4",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "prettier": "^2.2.1",
    "redis": "^3.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^4.2.3"
  }
}
